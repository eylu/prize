<h1>用户列表</h1>

<table>
  <thead>
    <tr>
      <th>姓名</th>
      <th>电话</th>
      <th>奖品ID</th>
      <th>奖品名称</th>

    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name  %></td>
        <td><%= user.phone %></td>
        <td><%= user.award_id %></td>
        <td><%= user.award_name %></td>
      </tr>
    <% end %>
  </tbody>
</table>



